<h1>Listing h1s</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @h1s.each do |h1| %>
  <tr>
    <td><%= h1.name %></td>
    <td><%= h1.age %></td>
    <td><%= link_to 'Show', h1 %></td>
    <td><%= link_to 'Edit', edit_h1_path(h1) %></td>
    <td><%= link_to 'Destroy', h1, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

